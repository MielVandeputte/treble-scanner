{
  "name": "treble-scanner",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "format:check": "prettier --check \"**/*.{js,tsx,ts,css,jsx,json}\"",
    "format:fix": "prettier --write \"**/*.{js,tsx,ts,css,jsx,json}\"",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "npm run format:check && npm run lint:check",
    "fix": "npm run format:fix && npm run lint:fix",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal-2023 public/logo.svg",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fontsource/proza-libre": "^5.2.5",
    "clsx": "^2.1.1",
    "motion": "^12.16.0",
    "qr-scanner": "^1.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.57.0",
    "react-router-dom": "^7.6.1",
    "store2": "^2.14.4"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/parser": "^8",
    "@vite-pwa/assets-generator": "^1",
    "@vitejs/plugin-react": "^4",
    "autoprefixer": "^10",
    "eslint": "^9",
    "eslint-config-prettier": "^10",
    "eslint-plugin-import": "^2",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^5",
    "eslint-plugin-react-refresh": "^0",
    "eslint-plugin-unicorn": "^59",
    "postcss": "^8",
    "prettier": "^3",
    "prettier-plugin-tailwindcss": "^0",
    "typescript": "^5",
    "typescript-eslint": "^8",
    "vite": "^6",
    "vite-plugin-pwa": "^1"
  }
}
